@(Html.Telerik().ScriptRegistrar().jQueryValidation(true)
    .DefaultGroup(group => 
        group.DefaultPath("~/Scripts")
                                    //.Add("jquery-ui-1.8.17.custom.min.js")
                                    .Add("bootstrap.min.js")
                                    .Add("jquery.validate.min.js")
                                    .Add("jquery.validate.unobtrusive.min.js")
                                    .Add("MyScripts.js")
                                    .Combined(false)
                                    .Compress(true)
                                    .CacheDurationInDays(365))
                                    )
